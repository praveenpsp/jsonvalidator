{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "firstname": {
      "type": "string"
    },
    "gender": {
      "type": "string"
    },
    "age": {
      "type": "integer"
    },
    "address": {
      "type": "object",
      "properties": {
        "address": {
          "type": "string"
        },
        "pincode": {
          "type": "integer"
        }
      },
      "additionalProperties": true,
      "required": [
        "address",
        "pincode"
      ]
    },
    "phone": {
      "type": "string"
    },
    "hobby": {
      "type": "array",
      "items": [
        {
          "type": "string"
        },
        {
          "type": "string"
        },
        {
          "type": "string"
        }
      ]
    }
  },
  "additionalProperties": true,
  "required": [
    "firstname",
    "gender",
    "age",
    "address",
    "phone",
    "hobby"
  ]
}